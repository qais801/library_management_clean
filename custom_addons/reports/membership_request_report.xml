<odoo>
  <!-- تقرير فردي لطلب عضوية واحد -->
  <report
      id="membership_request_report"
      model="library.membership.request"
      string="Membership Request Report"
      report_type="qweb-pdf"
      name="library_management.membership_request_report_template"
      file="library_management.membership_request_report_template"
      print_report_name="'Membership Request - %s' % (object.partner_id.name)"
  />

  <!-- تقرير متعدد لكل الطلبات -->
  <report
      id="membership_requests_report"
      model="library.membership.request"
      string="All Membership Requests Report"
      report_type="qweb-pdf"
      name="library_management.membership_requests_report_template"
      file="library_management.membership_requests_report_template"
      print_report_name="'Membership Requests Report'"
  />
</odoo>
